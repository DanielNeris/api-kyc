{
  "name": "api-drag-and-drop",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate --config drizzle.config.ts",
    "db:migrateprod": "NODE_ENV=production tsx .migrations/migrate.ts",
    "build": "tsc --build",
    "start": "yarn db:migrateprod && node dist/http/server.js",
    "dev": "yarn db:migrate && tsx watch --env-file .env src/http/server.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.5.4",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/static": "^8.0.3",
    "@paralleldrive/cuid2": "^2.2.2",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.22.7",
    "drizzle-orm": "^0.33.0",
    "fastify": "^5.0.0",
    "fastify-type-provider-zod": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "postgres": "^3.4.4",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}